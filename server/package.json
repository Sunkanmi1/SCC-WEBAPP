{
  "name": "supreme-court-api",
  "version": "1.0.0",
  "description": "API for searching Supreme Court cases from African countries via Wikidata",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch './**/*.ts' --exec node --loader ts-node/esm server.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "lint": "eslint ."
  },
  "engines": {
    "node": "^18"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
